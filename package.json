{
  "name": "rbsx",
  "version": "1.0.0",
  "description": "React, Bootstrap, SASS Boilerplate",
  "homepage": "https://rbsx.moquette.us",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moquette/rbsx"
  },
  "author": "Joaquin A. Moquette <me@moquette.us> (https://www.moquette.us/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moquette/rbsx/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint 'src/**/*.{js,jsx,json}'",
    "lint:fix": "eslint . --fix",
    "format": "yarn prettier --write --loglevel log .",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "predeploy": "yarn run build",
    "test": "react-scripts test",
    "prepare": "husky install",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "yarn format",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "bootstrap": "^5.0.0-beta3",
    "bootstrap-icons": "^1.4.1",
    "eslint-plugin-cypress": "^2.11.2",
    "gh-pages": "^3.1.0",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
